{
    "root": "",
    "title": "Styleguide",
    "gitbook": "3.2.2",
    "plugins": ["-highlight", "mathjax", "prism", "custom-favicon", "theme-api"],
    "pluginsConfig": {
        "parts-autoindex": {
            "level": "1",
            "trim": "0"
        },
        "favicon": "assets/favicon.ico",
        "theme-api": {
            "theme": "dark",
            "split": true,
            "languages": [
            {
                "lang":"java",
                "name": "Java",
                "default": "true"
            },
            {
                "lang": "cpp",
                "name": "C++"
            }
            ]
        }
    },
    "styles": {
        "website": "styles/website.css"
    }
}